<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog title="Select Lot Number" contentHeight="250px" contentWidth="400px">
        <FlexBox height="125px" alignItems="Center" justifyContent="Center">
            <Label text="Select Lot Number" />
            <Input id="idInpLotNo" showValueHelp="true" valueHelpRequest="onLotNumHelpRequest" />
        </FlexBox>
        <endButton>
            <Button text="Select and Proceed" press="onCloseLotNumberDialog" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
