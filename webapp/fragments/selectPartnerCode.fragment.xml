<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Partner Code Found"
		title="Select Partner Code"
		search="handleSearchPartnerCode"
		confirm="handleConfirmPartnerCode"
		cancel="handleClosePartnerCode"
		items="{
			path : '/ProdOrderPartIdSet'
		}">
		<ColumnListItem type="Active">
			<cells>
				<Text text="{ProductCode}" />
                <Text text="{ProdCodeDesc}" />
				<Text text="{PartnerCode}" />
                <Text text="{ParnerCodeDes}"/>
				<Text text="{ParentPartNumber}" />
                <Text text="{PartNumberDesc}" />
                <Text text="{ProductSeqFrom}" />
                <Text text="{ProductSeqTo}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="Product Code" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Product Code Description" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Partner Code" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Partner Code Description" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Parent Part No" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Part No Description" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Product Sequence From" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Product Sequence To" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>