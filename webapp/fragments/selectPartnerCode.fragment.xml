<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Partner Code Found"
		title="Select Partner Code"
		search="handleSearchPartnerCode"
		confirm="handleConfirmPartnerCode"
		cancel="handleClosePartnerCode"
		items="{
			path : '/ProductCollection'
		}">
		<ColumnListItem type="Active">
			<cells>
				<ObjectIdentifier title="{Name}" text="{ProductId}" />
				<ObjectIdentifier title="{MainCategory}" text="{Category}" />
				<Text/>
                <Text/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="Product Code" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Partner Code" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Parent Part" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="Product Sequence From" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>