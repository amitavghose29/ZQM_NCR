<core:FragmentDefinition  xmlns:m="sap.m" xmlns:core="sap.ui.core" xmlns="sap.ui.comp.valuehelpdialog" xmlns:fb="sap.ui.comp.filterbar">
	<!--<SelectDialog title="Copy NC" items="{sample>/tablelist}" liveChange="onSearchOrder" confirm="_configValueHelpDialogOrder"
		cancel="_handleValueHelpClose">
		<StandardListItem highlight="Information" title="{sample>Id}" info="{Ktext}"/>
	</SelectDialog>-->
    <ValueHelpDialog
        title="Copy NC"
        ok=".onValueHelpOkNCPress"
        cancel=".onValueHelpCancelNCPress"
        afterClose=".onValueHelpAfterNCClose"
        supportMultiselect="false"
        supportRanges="true"
        key="ProductId"
        descriptionKey="Name"
    >
        <filterBar>
            <fb:FilterBar advancedMode="true" search="onFilterBarSearch">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="ProductId"
                        label="NC Number"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input name="ProductId" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Serial Number"
                        label="Serial Number"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input name="Serial Number" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Aircraft"
                        label="Aircraft"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <m:Input name="Aircraft" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </filterBar>
    </ValueHelpDialog>
</core:FragmentDefinition>
