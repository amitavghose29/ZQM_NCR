	xmlns:fb="sap.ui.comp.filterbar">
	<Dialog  title="Production Order Search" class="vhdialogPOSearchstyle">
		<VBox class="sapUiSizeCompact">
			<!--	<fb:FilterBar id="filterbar" reset="onReset" search="onUserSearch" useToolbar="false" showGoOnFB="true">
				<fb:filterItems>
					<fb:FilterItem name="A" label="Product Sequence">
						<fb:control>
							<Input id="idConnector" width="130px" required="true" showValueHelp="true" valueHelpRequest="onValueHelpConnectors"/>
						</fb:control>
					</fb:FilterItem>
					<fb:FilterItem name="B" label="Part No.">
						<fb:control>
							<Input id="idUser" width="130px"/>
						</fb:control>
					</fb:FilterItem>
					<fb:FilterItem name="C" label="NLP">
						<fb:control>
							<Input id="idFname" width="130px"/>
						</fb:control>
					</fb:FilterItem>
					
				</fb:filterItems>
			</fb:FilterBar>-->
			<!--		</f:content>-->
			<!--	</f:DynamicPageHeader>-->
			<!--</f:header>-->
			<!-- DynamicPage Content -->
			<!--<f:content>-->
            <!-- onsearch   onsearch replaced -->
			<fb:FilterBar search="_onProdOrdSearchGo" showRestoreButton="false">
				<fb:filterGroupItems>
					<fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A1" label="Product Sequence">
						<fb:control>
							<Input id="idFlBarPrOrdVhPrdseq" type="Text" showSuggestion="true" suggestionItems="{oAircrafttNoModel>/}" valueHelpRequest="helpRequestProduct">
                            <suggestionItems>
									<core:Item text="{oAircrafttNoModel>Value}"/>
								</suggestionItems>
							</Input>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="B1" label="Part No. Affected">
						<fb:control>
                        <!-- suggestionItems="{ path : '/f4_genericSet',
                            filters : [{   
                                 path : 'Key',                        
                                 operator : 'EQ',
                                 value1 : 'PEFF'
                            }]
                        }"
                        suggest="onHandleSuggestPartNo"  -->
							<Input id="idFlBarPrOrdVhPartNo" showSuggestion="true" 
                                    suggestionItems="{oProdOrdPartSuggModel>/}" 
                                    valueHelpRequest="_onValueHelpReqPartNo" showValueHelp="true">
								<suggestionItems>
									<core:Item text="{oProdOrdPartSuggModel>Value}" />
								</suggestionItems>
							</Input>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="C1" label="NLP">
                        <fb:control>
                            <Input id ="idFlBarPrOrdVhnlp" type="Text" showSuggestion="true" suggestionItems="{oProdOrdNLPSuggModel>/}" valueHelpRequest="_onValueHelpReqNLP" showValueHelp="true">
								<suggestionItems>
									<core:Item text="{oProdOrdNLPSuggModel>Value}" />
								</suggestionItems>
							</Input>
                        </fb:control>
                     </fb:FilterGroupItem>
                     <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="D1" label="Work Instruction">
						<fb:control>
							<Input id="idFlBarPrOrdVhWrkIns" type="Text" showSuggestion="true" showValueHelp="true" suggestionItems="{oProdOrdWISuggModel>/}" valueHelpRequest="_onValueHelpReqWorkIns">
                                <suggestionItems>
									<core:Item text="{oProdOrdWISuggModel>Value}" />
								</suggestionItems>
							</Input>
						</fb:control>
					</fb:FilterGroupItem>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="E1" label="Closed Orders">
						<fb:control>
							<CheckBox id="idFlBarPrdOrdVhClsOrd"/>
						</fb:control>
					</fb:FilterGroupItem>
				    </fb:filterGroupItems>
			</fb:FilterBar>
			<Table id="idProdOrderTable" inset="false" items="{oPOModel>/}" mode="SingleSelectMaster" selectionChange="handleCloseUserValueHelpProdOrd"
				itemPress="handleCloseUserValueHelpProdOrd">
				<columns>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Plant"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Order"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Prod Seq."/>
					</Column>
                    <Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Prod Order Item"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Type"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Parent"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="NLP"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Start Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="End Date"/>
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="Issue Qty."/>
					</Column>
				</columns>
				<items>
					<ColumnListItem vAlign="Middle" type="Active" press="_configValueHelpDialogOrderPartNo">
						<cells>
							<Text text="{oPOModel>Plant}"/>
							<Text text="{oPOModel>Order}"/>
							<Text text="{oPOModel>ProductSequence}"/>
                            <Text text="{oPOModel>ProdOrdItem}"/>
							<Text text="{oPOModel>Type}"/>
							<Text text="{oPOModel>PartNumber}"/>
							<Text text="{oPOModel>NLP}"/>
							<Text text="{oPOModel>StartDate}"/>
							<Text text="{oPOModel>EndDate}"/>
							<Text text="{oPOModel>IssueQty}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
			<!--	</f:content>-->
			<!--</f:DynamicPage>-->
		</VBox>
		<endButton>
			<Button text="Close" press="onCloseProdOrdPopup"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>