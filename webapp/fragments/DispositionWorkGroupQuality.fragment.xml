<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" class="sapUiSizeCompact">
	<Dialog title="WorkGroup" resizable="true" contentHeight="250px" contentWidth="250px">
        <Table items="{WorkGroupQual>/}" itemPress="onQualWorkGrpItemSel">
            <columns>
                <Column minScreenWidth="Tablet" demandPopin="true"> 
                    <Text text="WorkGroup" />
                </Column>
				<Column minScreenWidth="Tablet" demandPopin="true">
                    <Text text="Description" />
                </Column>
            </columns>
            <items>
                <ColumnListItem type="Active">
                    <cells>
                        <Text text="{WorkGroupQual>Value}" />
						<Text text="{WorkGroupQual>Description}" />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <endButton>
            <Button text="Close" press="onCloseWrkGrpQuality" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
