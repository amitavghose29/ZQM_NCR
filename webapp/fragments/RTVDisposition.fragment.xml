		search="handleSearchRTV"
		cancel="onCloseRTVPopout"
		items="{
			path : '/'
		}">
		<ColumnListItem type="Active">
			<cells>
				<Text text="{}" />
                <Text text="{}" />
				<Text text="{}" />
                <Text text="{}"/>
				<Text text="{}" />
                <Text text="{}" />   
			</cells>
		</ColumnListItem>
		<columns>
			<Column>
				<header>
					<Text text="Discrepancy" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Ret PO No." />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="PO Item" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Outbound Delivery" />
				</header>
			</Column>
			<Column>
				<header>
					<Text text="RMA" />
				</header>
			</Column>
            <Column>
				<header>
					<Text text="Release Notes" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>