<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:layout="sap.ui.layout" xmlns:f="sap.f" xmlns:fb="sap.ui.comp.filterbar">
    <Dialog id="idPOHelpDialog" title="Purchase Order Search" class="vhdialogPOSearchstyle">
        <VBox class="sapUiSizeCompact">
            <fb:FilterBar reset="onPOfbReset" search="onPOfbSearch" showRestoreButton="true" showClearButton="true">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="A1" label="ASN number">
                        <fb:control>
                            <Input id="idASNNo" type="Text" valueHelpRequest="_onValueHelpReqtASNNumber" showValueHelp="true" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="B1" label="Inbound delivery">
                        <fb:control>
                            <Input id="idInboundDelivery" valueHelpRequest="_onValueHelpReqInboundDelivery" showValueHelp="true" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="C1" label="Part Number">
                        <fb:control>
                            <Input id="idFlBarPOVhPartNo" type="Text" valueHelpRequest="_onValueHelpReqPartNo" showValueHelp="true" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="D1" label="NC Number">
                        <fb:control>
                            <Input type="Text" valueHelpRequest="_onValueHelpReqNCNo" showValueHelp="true" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="E1" label="Discrepancy Number">
                        <fb:control>
                            <Input type="Text" valueHelpRequest="_onValueHelpReqDiscrepancyNo" showValueHelp="true" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem visibleInFilterBar="true" groupName="G1" groupTitle="Group1" name="F1" label="RMA number">
                        <fb:control>
                            <Input type="Text" valueHelpRequest="_onValueHelpReqRMANo" showValueHelp="true" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
            <Table id="idPurOrdTable" inset="false" items="{oPrModel>/}" mode="SingleSelectMaster" selectionChange="handleClosePOUserValueHelp" itemPress="handleClosePOUserValueHelp">
                <columns>
                    <Column minScreenWidth="Tablet" demandPopin="true">
                        <Text text="PO ID" />
                    </Column>
                    <Column minScreenWidth="Tablet" demandPopin="true">
                        <Text text="PO Description" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem vAlign="Middle" type="Active">
                        <cells>
                            <Text text="{oPrModel>Value}" />
                            <Text text="{}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </VBox>
        <endButton>
            <Button text="Close" press="_handlePOValueHelpClose" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>